csvspec: {
  name: "test_required_field_value"
  filename: "test_required_field_value.txt"

  column: {
    name: "float_optional_test"
    type: { type: FLOAT }
    }

  column: {
    name: "float_required_test"
    type: { type: FLOAT }
    value_required: true
    }

  column: {
      name: "integer_optional_test"
      type: { type: INTEGER }
    }

  column: {
    name: "integer_required_test"
    type: { type: INTEGER }
    value_required: true
    }

  column: {
    name: "color_optional_test"
    type: { type: COLOR }
    }

  column: {
      name: "color_required_test"
      type: { type: COLOR }
      matchregexp: "[0-9a-fA-F]{6}"
      value_required: true
    }

  column: {
      name: "unspecified_optional_test"
    }

    column: {
      name: "unspecified_required_test"
      value_required: true
    }

  column: {
      name: "text_optional_test"
      type: { type: TEXT }
    }

    column: {
      name: "text_required_test"
      type: { type: TEXT }
      value_required: true
    }
}